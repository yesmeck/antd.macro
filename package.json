{
  "name": "antd.macro",
  "version": "1.0.1",
  "description": "A babel macro that helps you import antd's style.",
  "keywords": [
    "antd",
    "babel-plugin-macros"
  ],
  "main": "lib/index.js",
  "author": "Wei Zhu <yesmeck@gmail.com>",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "jest",
    "watch": "tsc -w",
    "prebuild": "rimraf lib",
    "build": "tsc",
    "prepack": "npm run test && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/traverse": "^7.4.4",
    "@babel/types": "^7.4.4",
    "@types/jest": "^24.0.12",
    "@types/node": "^11.13.9",
    "antd": "^3.16.6",
    "babel-plugin-macros": "^2.5.1",
    "babel-plugin-tester": "^6.0.1",
    "husky": "^2.2.0",
    "jest": "^24.7.1",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@babel/helper-module-imports": "^7.0.0"
  },
  "peerDependencies": {
    "antd": ">=3.0.0 || >=4.0.0",
    "babel-plugin-macros": ">2.0.0"
  }
}
